{
  "name": "journeysaveparser",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && npm run deploy",
    "production": "vue-tsc --noEmit && vite build --base=/Journey2/ && npm run deploy",
    "deploy": "node buildScripts/deployScript buildScripts/include dist",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/node": "^17.0.33",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.15.0",
    "eslint-plugin-vue": "^8.7.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7"
  }
}
